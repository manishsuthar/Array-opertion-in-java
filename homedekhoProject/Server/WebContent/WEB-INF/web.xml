<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>homeDekho</display-name>
  <filter>
    <filter-name>CorsFilter</filter-name>
    <filter-class>org.apache.catalina.filters.CorsFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CorsFilter</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>FileHandler</servlet-name>
    <servlet-class>UploadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileHandler</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>AddListData</servlet-name>
    <servlet-class>InfomationAddServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddListData</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GettingRequest</servlet-name>
    <servlet-class>GettingDataModal</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GettingRequest</servlet-name>
    <url-pattern>/getting</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetView</servlet-name>
    <servlet-class>GetViewData</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetView</servlet-name>
    <url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>GetImage</servlet-name>
    <servlet-class>ImageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GetImage</servlet-name>
    <url-pattern>/image</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ContectAdd</servlet-name>
    <servlet-class>ContectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ContectAdd</servlet-name>
    <url-pattern>/contectAdd</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>SearchData</servlet-name>
    <servlet-class>SearchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchData</servlet-name>
    <url-pattern>/Search</url-pattern>
  </servlet-mapping>
</web-app>